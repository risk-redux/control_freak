<% if control %>
<span class="giant badge rounded-pill text-bg-success">
  <%= level[0] %> <i class='fa fa-check-circle' title='Selected'></i>
</span>
<% else %>
<span class="giant badge rounded-pill text-bg-secondary">
  <%= level[0] %> <i class='fa fa-times-circle' title='Not selected'></i>
</span>
<% end %>